<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>AllReview</servlet-name>
        <servlet-class>controller.AllReview</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SpecificReview</servlet-name>
        <servlet-class>controller.SpecificReview</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AllReview</servlet-name>
        <url-pattern>/AllReview</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SpecificReview</servlet-name>
        <url-pattern>/SpecificReview</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>AllReview</welcome-file>
        <welcome-file>MovieReview.jsp</welcome-file>        
    </welcome-file-list>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
